{
  "expo": {
    "name": "Flex Aura",
    "slug": "flex-aura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mascot/flex_aura_logo_no_bg.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/mascot/flex_aura_logo_no_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mascot/flex_aura_logo_no_bg.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "notification": {
      "icon": "./assets/mascot/flex_aura_logo_no_bg.png",
      "color": "#B7FCE7",
      "androidMode": "default",
      "androidCollapsedTitle": "Flex Aura"
    },
    "web": {
      "favicon": "./assets/mascot/flex_aura_logo_no_bg.png"
    }
  }
}
